@misc{amini2022,
  title = {Hyperlocal Super-Learned {{PM2}}.5 Components across the Contiguous {{US}}},
  author = {Amini, Heresh and {Danesh-Yazdi}, Mahdieh and Di, Qian and Requia, Weeberb and Wei, Yaguang and {Abu-Awad}, Yara and Shi, Liuhua and Franklin, Meredith and Kang, Choong-Min and Wolfson, Jack and James, Peter and Habre, Rima and Zhu, Qiao and Apte, Joshua and Andersen, Zorana and {kloog}, Itai and Dominici, Francesca and Koutrakis, Petros and Schwartz, Joel},
  year = {2022},
  month = jun,
  issn = {2693-5015},
  doi = {10.21203/rs.3.rs-1745433/v1},
  urldate = {2025-06-27},
  abstract = {Particulate matter with aerodynamic diameter less than 2.5 {$\mu$}m (PM2.5) increases mortality and morbidity.1,2 PM2.5 is composed of a mixture of chemical components that vary across space and time.3 Due to limited hyperlocal data availability, less is known about health risks of PM2.5 components, their US-wide exposure disparities, or which species are driving the biggest intra-urban changes in PM2.5 mass. Here, we developed the first national super-learned models across the US for hyperlocal estimation of annual mean elemental carbon (EC), ammonium (NH4+), nitrate (NO3-), organic carbon (OC), and sulfate (SO42-) concentrations across 3,535 urban areas at a 50-m spatial resolution, and at a 1-km resolution for non-urban areas from 2000 to 2019. Using ensembles of machine learning models and {\textasciitilde}82 billion predictions across 20 years, hyperlocal super-learned PM2.5 components are now available for further research. We found remarkable spatiotemporal intra-urban and inter-urban variabilities in PM2.5 components. We anticipate our work to be a critical milestone for conducting new studies on individual and combined health risks of PM2.5 components, environmental justice analysis, or understanding fine-scale spatiotemporal variabilities of PM2.5 composition. Urban planners and regulators may also use these predictions for selecting locations of new daycares, schools, nursing homes, or air-quality monitors.},
  file = {/Users/calensmith/Zotero/storage/WV2NKCNT/Amini et al. - 2022 - Hyperlocal super-learned PM2.5 components across the contiguous US.pdf}
}

