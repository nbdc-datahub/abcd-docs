  title = {International {{Study}} of {{Asthma}} and {{Allergies}} in {{Childhood}} ({{ISAAC}}): {{Rationale}} and Methods},
  shorttitle = {International {{Study}} of {{Asthma}} and {{Allergies}} in {{Childhood}} ({{ISAAC}})},
  author = {Asher, Mi and Keil, U and Anderson, Hr and Beasley, R and Crane, J and Martinez, F and Mitchell, Ea and Pearce, N and Sibbald, B and Stewart, Aw and Et, {\relax Al}.},
