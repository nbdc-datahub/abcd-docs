@article{wing2022,
  title = {Recommendations for {{Identifying Valid Wear}} for {{Consumer-Level Wrist-Worn Activity Trackers}} and {{Acceptability}} of {{Extended Device Deployment}} in {{Children}}},
  author = {Wing, David and Godino, Job G. and Baker, Fiona C. and Yang, Rongguang and Chevance, Guillaume and Thompson, Wesley K. and Reuter, Chase and Bartsch, Hauke and Wilbur, Aimee and Straub, Lisa K. and Castro, Norma and Higgins, Michael and Colrain, Ian M. and De Zambotti, Massimiliano and Wade, Natasha E. and Lisdahl, Krista M. and Squeglia, Lindsay M. and Ortigara, Joseph and Fuemmeler, Bernard and Patrick, Kevin and Mason, Michael J. and Tapert, Susan F. and Bagot, Kara S.},
  year = {2022},
