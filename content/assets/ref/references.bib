@article{rice1995,
  title = {Comparison of Direct Interview and Family History Diagnoses of Alcohol Dependence},
  author = {Rice, J. P. and Reich, T. and Bucholz, K. K. and Neuman, R. J. and Fishman, R. and Rochberg, N. and Hesselbrock, V. M. and Nurnberger, J. I. and Schuckit, M. A. and Begleiter, H.},
  year = {1995},
