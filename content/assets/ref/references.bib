  keywords = {Adolescent,Adolescent Behavior,Authoritarianism,Child,Child Behavior Disorders,Data Collection,Discriminant Analysis,Factor Analysis,Female,Humans,Internal-External Control,Male,Models,Parent-Child Relations,Psychological,Psychology,Statistical}
